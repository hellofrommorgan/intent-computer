{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "",
  "installCommand": "",
  "outputDirectory": "site",
  "framework": null,
  "cleanUrls": true,
  "redirects": [
    { "source": "/context", "destination": "/system", "statusCode": 301 },
    { "source": "/skills", "destination": "/system", "statusCode": 301 },
    { "source": "/loop", "destination": "/system", "statusCode": 301 }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
